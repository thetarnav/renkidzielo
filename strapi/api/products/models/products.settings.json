{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "name": "Products",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": false,
    "draftAndPublish": true
  },
  "attributes": {
    "title": {
      "type": "string",
      "default": "Tytu≈Ç nowego produktu...",
      "required": true,
      "maxLength": 50,
      "unique": true
    },
    "number": {
      "type": "integer"
    },
    "thumbnail": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": true,
      "pluginOptions": {}
    },
    "slides": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "description": {
      "type": "richtext"
    },
    "table": {
      "type": "text",
      "regex": ".*: .*"
    },
    "shopLink": {
      "type": "string",
      "regex": "[-a-zA-Z0-9@:%._\\+~#=]{1,256}\\.[a-zA-Z0-9()]{1,6}\\b([-a-zA-Z0-9()@:%_\\+.~#?&//=]*)"
    },
    "isAvailable": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "timestamp": {
      "type": "biginteger"
    },
    "views": {
      "type": "integer",
      "default": 0
    },
    "category": {
      "via": "products",
      "model": "categories"
    },
    "ties": {
      "collection": "ties",
      "via": "products"
    },
    "dailyViews": {
      "type": "json"
    },
    "popularity": {
      "type": "decimal",
      "default": 0
    },
    "year": {
      "type": "integer"
    }
  }
}
